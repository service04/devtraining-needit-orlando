        <condition/>
        <description>Set the total date create for new records.</description>
        <field>active</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Load Total Date Create Field</name>
        <order/>
        <script><![CDATA[function onLoad() {
   //Type appropriate comment here, and begin script below
if(g_form.getValue('u_total_date')== '')
{
g_form.setValue('u_total_date','0');
}
else 
{
var enddate = new date();
var total = abs(enddate - g_form.getValue('u_total_date'));
Math.floor(total / (60*60*24));
}
	// Do your operations	
}]]></script>
